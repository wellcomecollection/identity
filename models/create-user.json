{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Create User Schema",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "firstName": {
      "type": "string",
      "minLength": 1
    },
    "lastName": {
      "type": "string",
      "minLength": 1
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 1
    }
    },
  "required": [
    "title",
    "firstName",
    "lastName",
    "email",
    "password"
  ],
  "additionalProperties": false
}
