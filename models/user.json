{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "User Schema",
  "type": "object",
  "properties": {
    "userId": {
      "type": "number",
      "minimum": 0,
      "multipleOf": 1.0
    },
    "barcode": {
      "type": "string",
      "minLength": 1
    },
    "firstName": {
      "type": "string",
      "minLength": 1
    },
    "lastName": {
      "type": "string",
      "minLength": 1
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "emailValidated": {
      "type": "boolean"
    },
    "locked": {
      "type": "boolean"
    },
    "creationDate": {
      "type": "string",
      "format": "date-time"
    },
    "updatedDate": {
      "type": "string",
      "format": "date-time"
    },
    "lastLoginDate": {
      "type": "string",
      "format": "date-time"
    },
    "lastLoginIp": {
      "oneOf": [
        {
          "type": "string",
          "format": "ipv4"
        },
        {
          "type": "string",
          "format": "ipv6"
        }
      ]
    },
    "totalLogins": {
      "type": "number",
      "minimum": 0,
      "multipleOf": 1.0
    },
    "deleteRequested": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "userId",
    "barcode",
    "firstName",
    "lastName",
    "email",
    "emailValidated",
    "locked",
    "creationDate"
  ],
  "additionalProperties": true
}
