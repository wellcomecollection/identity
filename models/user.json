{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "User Schema",
  "type": "object",
  "properties": {
    "patronId": {
      "type": "number",
      "minimum": 0,
      "multipleOf": 1.0
    },
    "barcode": {
      "type": "string",
      "minLength": 1
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "firstName": {
      "type": "string",
      "minLength": 1
    },
    "lastName": {
      "type": "string",
      "minLength": 1
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "emailValidated": {
      "type": "boolean"
    },
    "locked": {
      "type": "boolean"
    },
    "creationDate": {
      "type": "string",
      "format": "date-time"
    },
    "lastLogin": {
      "type": "string",
      "format": "date-time"
    },
    "lastLoginIp": {
      "anyOf": [
        {
          "type": "string",
          "format": "ipv4"
        }, {
          "type": "string",
          "format": "ipv6"
        }
      ]
    },
    "totalLogins": {
      "type": "number",
      "minimum": 0,
      "multipleOf": 1.0
    }
  },
  "required": [
    "patronId",
    "barcode",
    "title",
    "firstName",
    "lastName",
    "email",
    "emailValidated",
    "locked",
    "creationDate",
    "lastLogin",
    "lastLoginIp",
    "totalLogins"
  ],
  "additionalProperties": false
}
