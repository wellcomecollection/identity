{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Item Request List Schema",
  "type": "object",
  "properties": {
    "requests": {
      "type": "array",
      "items": {
        "properties": {
          "workId": {
            "type": "string",
            "minLength": 1
          },
          "itemId": {
            "type": "string",
            "minLength": 1
          },
          "type": {
            "const": "ItemRequest"
          }
        }
      }
    },
    "totalResults": {
      "type": "integer",
      "minimum": 0
    },
    "type": {
      "const": "ItemRequestList"
    }
  },
  "required": ["requests", "totalResults", "type"],
  "additionalProperties": false
}
